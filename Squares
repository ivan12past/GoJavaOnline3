package javaonline3.hw4;

public class Squares {
    public double countRectangleSquare( double firstSide , double secondSide){
        return    firstSide * secondSide;
    }
    public double countCircleSquare(double radius){
        return   Math.PI * radius * radius;
    }
    public double countTriangleSwuare(double height, double side){
        return   (height * side)/2;
    }

}
