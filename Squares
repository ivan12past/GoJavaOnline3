public class Squares {
    static final double PI = 3.14;
    public double rectSquare( double firstSide , double secondSide){
        double S1 = firstSide * secondSide;
        return S1;
    }
    public double circSquare(double radiusCirc){
        double S2 = PI * radiusCirc * radiusCirc;
        return S2;
    }
    public double triSquare(double heightTri , double sideTri){
        double S3 = (heightTri * sideTri)/2;
        return S3;
    }

}
